<span class = "heading">
    <h1 align = "center"> Welcome to <<%= @user.username %>'s page </h1>
</span>

<div class = "row">
    <div class = "col-md-4 col-md-offset-4 center">
        <%= gravatar_for @user, size: 75 %>
    </div>
</div>
<h4 align="center"><%= @user.username %>'s Clients </h4>

<div class = "listing">
<%= render 'clients/client', obj: @user.clients %>
</div>

<div class = "center">
    <%= will_paginate @user_clients %>
</div>